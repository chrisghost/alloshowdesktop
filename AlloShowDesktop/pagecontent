	<script type="text/javascript">
	var viewsloaded = false;
	var commentsloaded = false;
	var favoritesloaded = false;

	function loadvideos(type)
	{
	  document.getElementById('featured_html').style.display = 'none';
	  document.getElementById('views_html').style.display = 'none';
	  document.getElementById('comments_html').style.display = 'none';
	  document.getElementById('favorites_html').style.display = 'none';

	  if (type == '1')
	  {
	    document.getElementById('featured_html').style.display = '';
	  }
	  if (type == '2')
	  {
	    document.getElementById('views_html').style.display = '';
		if (!viewsloaded)
		{
		  document.getElementById('videoloader').style.display='';
		  setTimeout("document.getElementById('vidloader').startLoader();",100);
		  document.getElementById('ajaxtype').value = 'views';
		  document.getElementById('ajaxfrm').submit();
		}
	  }
	  if (type == '3')
	  {
	    document.getElementById('comments_html').style.display = '';
		if (!commentsloaded)
		{
		  document.getElementById('videoloader').style.display='';
		  setTimeout("document.getElementById('vidloader').startLoader();",100);
		  document.getElementById('ajaxtype').value = 'comments';
		  document.getElementById('ajaxfrm').submit();
		}
	  }
	  if (type == '4')
	  {
	    document.getElementById('favorites_html').style.display = '';
		if (!favoritesloaded)
		{
		  document.getElementById('videoloader').style.display='';
		  setTimeout("document.getElementById('vidloader').startLoader();",100);
		  document.getElementById('ajaxtype').value = 'favorites';
		  document.getElementById('ajaxfrm').submit();
		}
	  }
	}

	
	</script>
	



