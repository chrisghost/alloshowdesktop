	function autoSenseLang() {
		try {
			var url = window.location.href;
			var locale = getLocale();
			if (undefined == locale) {
				return;
			}
			if ('en_US' != locale && url.indexOf('/' + locale + '/') == -1) {
				window.location.href = '/' + locale;
			}
		}
		catch (error) {
			;
		}
	}

	function getLangFromURL() {
		var url = window.location.href;
		var lang = locales[0];
		for (var i=1; i < locales.length; i++) {
			if (url.indexOf('/' + locales[i] + '/') != -1) {
				lang = locales[i];
				break;
			}
		}
		return lang;
	}

	function getUnique(max) {
		var now = new Date();
		return (now.getMilliseconds() * Math.floor(Math.random()*max+1));
	}

	function isWin() {
		return (navigator.platform.indexOf('Win') != -1);
	}

	function isVista() {
		return (navigator.userAgent.indexOf("Windows NT 6.0") != -1);
	}

	function isMac() {
		return (navigator.platform.indexOf('Mac') != -1);
	}

	function isLinux() {
		return (navigator.platform.indexOf('Linux') != -1);
	}






	function addCacheBreakerToURL(url) {
		var ran_number= getUnique(8);
		var firstLetter = (url.indexOf('?')<0) ? '?' : '&';
		return url + firstLetter + 'bznetid=' + ran_number;
	}


	function logout()
	{
	  document.getElementById('logoutform').submit();
	}


	

	function changelang(id)
	{
		if (id == 0)
		{
			document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=en';
		}
		if (id == 1)
		{
			document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=de';
		}
		if (id == 2)
		{
			document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=fr';
		}
		if (id == 3)
		{
			document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=es';
		}
		if (id == 4)
		{
		document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=pt';
		}
		if (id == 5)
		{
			document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=nl';
		}
		if (id == 6)
		{
			document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=it';
		}
		if (id == 7)
		{
			document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=cn';
		}
		if (id == 8)
		{
			document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=ct';
		}
		if (id == 9)
		{
			document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=jp';
		}
		if (id == 10)
		{
			document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=kr';
		}
		if (id == 11)
		{
			document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=ru';
		}
		if (id == 12)
		{
			document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=fi';
		}
		if (id == 13)
		{
			document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=se';
		}
		if (id == 14)
		{
			document.location = 'http://www.megavideo.com/?v=FBK8L7MI&setlang=dk';
