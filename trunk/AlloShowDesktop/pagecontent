document.playerstate=1;
function changeplayerstate()
{
  if (document.playerstate)
  {
    // make small:
	document.getElementById('playermenu').style.width = '640px';
	document.getElementById('mvplayer').style.width	 = '640px';
	document.getElementById('mvplayer').style.height = '360px';
	document.playerstate=0;
	document.getElementById('pbut1').className = 'pl_butt1_s';
	document.getElementById('pbut2').className = 'pl_butt2_s';
	document.getElementById('pbut3').className = 'pl_butt3_s';
	if (document.cinemaon) document.getElementById('pbut4').className = 'pl_butt4b_s';
	else document.getElementById('pbut4').className = 'pl_butt4_s';
  }
  else
  {
    // make big:
	document.getElementById('playermenu').style.width = '974px';
	document.getElementById('mvplayer').style.width	 = '974px';
	document.getElementById('mvplayer').style.height = '574px';
	document.getElementById('pbut1').className = 'pl_butt1';
	document.getElementById('pbut2').className = 'pl_butt2';
	document.getElementById('pbut3').className = 'pl_butt3';
	if (document.cinemaon) document.getElementById('pbut4').className = 'pl_butt4b';
	else document.getElementById('pbut4').className = 'pl_butt4';
	document.playerstate=1;
  }
}


function embedvideo()
{

    document.getElementById('mcadunit').style.display = 'none';
  
  document.getElementById('related_new').style.display = 'none';
  document.getElementById('embed_video').style.display = '';

}

function closeembed()
{
  document.getElementById('embed_video').style.display='none';
}


function relatedvideos()
{
  if (document.relatedloaded)
  {
	  document.getElementById('related_new').style.display = '';
	  document.getElementById('embed_video').style.display = 'none';
  }
  else loadrelated();  

}

function closerelated()
{
  document.getElementById('related_new').style.display='none';
}

document.cinemaon=0;
function cinemamode()
{
  if (document.cinemaon)
  {
	if (document.playerstate) document.getElementById('pbut4').className = 'pl_butt4';
	else document.getElementById('pbut4').className = 'pl_butt4_s';
    document.cinemaon=0;
	document.getElementById('cinemamode').style.display='none';
	document.getElementById('playerdiv').style.zIndex = '0';
  }
  else
  {
	if (document.playerstate) document.getElementById('pbut4').className = 'pl_butt4b';
	else document.getElementById('pbut4').className = 'pl_butt4b_s';
    document.cinemaon=1;
	document.getElementById('playerdiv').style.zIndex = '100';
	document.getElementById('cinemamode').style.display='';
	changeOpac(65,'cinemamode');
  }
}
function changeOpac(opacity, id) { 
    var object = document.getElementById(id).style; 
    object.opacity = (opacity / 100); 
    object.MozOpacity = (opacity / 100); 
    object.KhtmlOpacity = (opacity / 100); 
    object.filter = "alpha(opacity=" + opacity + ")"; 
} 

function previewplayer()
{ 
  
  window.open('previewplayer/?v=POI8XPGS&width=' + ewidth + '&height=' + eheight + '&image=47d92273644541a326e22c98dbefeacd&new=',name,'width='+(ewidth)+',height='+(eheight)+',scrollbars=no,status=no,resizable=yes, toolbar=no'); 
    
}


function playercomplete()
{

}

function bodyload()
{
  document.bodyloaded=true;
}

function playerout()
{ 
  if (document.bodyloaded)
  {
	document.getElementById('mvplayer').hideNavigation();
  }
}



document.bodyloaded=false;


